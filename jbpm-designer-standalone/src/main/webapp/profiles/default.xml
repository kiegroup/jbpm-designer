    <plugin name="ORYX.Plugins.Layouter.EdgeLayouter" />
    <!-- to have a better container layout ? -->
    <plugin name="ORYX.Plugins.ContainerLayouter" />
    <!-- lane layouter -->
    <plugin name="ORYX.Plugins.DragTracker.LaneDragTracker" />
    <plugin name="ORYX.Plugins.DragTracker.PoolDragTracker" />
    <stencilsetextension
      namespace="http://oryx-editor.org/stencilsets/extensions/bpmncosts-2.0#" />
